#! /usr/bin/env bash

IPASS="/home/jippe/iPass"

if [[ "$OSTYPE" == "linux-gnu" ]]; then
	echo "Linux detected, continuing";
fi

cd $IPASS/src/project/unit_tests/;make build; make run;make clean>/dev/null
pause

